<?xml version='1.0' encoding='utf-8'?>
<widget id="SRB.FalabellaCuentaSueldo1" version="2.2.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cuenta Sueldo Banco Falabella</name>
    <description></description>
       
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Agencia Pucnic
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    
    <plugin name="org.apache.cordova.device" spec="0.2.3" source="pgb" />
    <plugin name="org.apache.cordova.geolocation" />
    <plugin name="nl.x-services.plugins.socialsharing" />

    <preference name="android-build-tool" value="ant" />
    <plugin name="phonegap-plugin-push" source="npm">
        <param name="SENDER_ID" value="564039352699" />
    </plugin>

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
